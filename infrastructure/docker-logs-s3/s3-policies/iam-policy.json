{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DockerLogsS3Upload",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:PutObjectAcl",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::prod-app.card2contacts.com",
        "arn:aws:s3:::prod-app.card2contacts.com/prod/backend/*",
        "arn:aws:s3:::prod-app.card2contacts.com/prod/db/*"
      ]
    }
  ]
}
